<?xml version="1.0" encoding="UTF-8"?>

<!--
    STC Log4J Configuration
    Edit this to change logging behavior.
    The production default is a simple stream to stdout,
    which is normally empty.
-->

<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%-5level %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <!-- Set level to INFO or DEBUG or TRACE for more logging.
         For production, use WARN.
    -->
    <Root level="WARN">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
